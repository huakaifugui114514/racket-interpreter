(class Animal ()
  (field name "Unknown")
  
  (constructor (n)
    (set this name n))
  
  (method speak () 
    (println "Animal sound")))

(class Dog (extends Animal)
  (method speak () 
    (println (string-append (get this name) " says: Woof!"))))

(define a (new Animal "Generic Animal"))
(send a speak)

(define d (new Dog "Buddy"))
(send d speak)