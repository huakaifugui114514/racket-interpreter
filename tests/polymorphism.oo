(class Shape ()
  (method area () 0))

(class Rectangle (extends Shape)
  (field width 0)
  (field height 0)
  
  (constructor (w h)
    (set this width w)
    (set this height h))
  
  (method area () 
    (* (get this width) (get this height))))

(class Circle (extends Shape)
  (field radius 0)
  
  (constructor (r)
    (set this radius r))
  
  (method area ()
    (* 3.14159 (get this radius) (get this radius))))

(define shapes (list (new Rectangle 5 10) (new Circle 7)))

; 多态调用
(for-each (lambda (s) (print (send s area))) shapes)
; 输出: 50 和 153.93791